<h1>Warehouse Name: <%= @warehouse.name %></h1>
<p>Location: <%= @warehouse.state %></p>

<h1>Items At This Warehouse</h1>
<% @warehouse.itemwarehouses.each do |item| %>
<p> <%= item.item.name %></p>
<p>  <%= item.item.price %></p>
<% end %>

<h1>All Items</h1>
<% @items.each do |item| %>
  <p><%= item.name %></p>
  <p><%= item.price %></p>
  <p><%= button_to "Add Item", "/warehouse/#{@warehouse.id}/item/#{item.id}", method: :post %></p>
<% end %>
